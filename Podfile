# Uncomment the next line to define a global platform for your project
platform :ios, '11.2'

target 'dvvy' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for dvvy
  pod 'Firebase/Core'
  pod 'Firebase/Firestore'
  pod 'Firebase/Auth'
  pod 'KeychainSwift'
  pod 'pop', '~> 1.0'

  target 'dvvyTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'dvvyUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
